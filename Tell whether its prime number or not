import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in) ;
        System.out.print(" Enter a number :");
        int n = sc.nextInt();
        int count=0;
     for(int i=2;i<=n/2;i++)
       {
           if(n%i==0)
           {
               count++;
               break;       
            }
       }
       if(count>0)
       System.out.print("The number you have entered is not prime");
       else
       System.out.print("The number you have entered is prime ");
    }
}

import java.util.*;
class Main
{
  public static void main (String[]args)
  {
    Scanner obj = new Scanner (System.in);
      System.out.print ("Enter the size: ");
    int size = obj.nextInt ();
    int a[] = new int[100];
    int sum = 0;

    for (int i = 0; i < size; i++)
      {
	a[i] = obj.nextInt ();
      }
    for (int i = 0; i < size; i++)
      {
	sum += a[i];
      }
    System.out.println (sum);
  }
}
